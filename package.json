{
	"name": "backend_formcam",
	"version": "1.0.0",
	"main": "dist/server.js",
	"license": "MIT",
	"type": "module",
	"engines": {
		"node": ">=24.0.0"
	},
	"scripts": {
		"dev": "tsx --watch --env-file=.env  src/server.ts",
		"build": "tsc --project tsconfig.json",
		"build-tsc": "tsc --project tsconfig.json",
		"start": "node dist/server.js",
		"railway-build": "yarn build",
		"railway-start": "yarn prisma migrate deploy && yarn start",
		"check-db": "node scripts/check-db.js",
		"postinstall": "yarn prisma generate"
	},
	"dependencies": {
		"@fastify/cors": "^10.0.2",
		"@fastify/jwt": "^9.0.3",
		"@fastify/swagger": "^9.5.1",
		"@fastify/swagger-ui": "^5.2.3",
		"@prisma/client": "^7.3.0",
		"bcrypt": "^6.0.0",
		"dotenv": "^17.2.4",
		"fastify": "^5.2.1",
		"fastify-type-provider-zod": "^6.1.0",
		"jsonwebtoken": "^9.0.2",
		"pino": "^10.1.0",
		"pino-pretty": "^13.1.2",
		"uuid": "^13.0.0",
		"zod": "^4.1.13"
	},
	"devDependencies": {
		"@biomejs/biome": "^1.9.4",
		"@types/bcrypt": "^6.0.0",
		"@types/node": "^25.2.2",
		"prisma": "^7.3.0",
		"tsup": "^8.5.0",
		"tsx": "^4.19.2",
		"typescript": "^5.7.3"
	},
	"packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e",
	"builds": [
		{
			"src": "dist/server.js",
			"use": "@vercel/node"
		}
	],
	"routes": [
		{
			"src": "/(.*)",
			"dest": "dist/server.js"
		}
	]
}
